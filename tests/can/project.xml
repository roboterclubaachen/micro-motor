    </repository>
    <cache>.lbuild_cache</cache>
  </repositories>

  <options>
    <option name=":build:build.path">build</option>
    <option name=":build:openocd.cfg">../openocd.cfg</option>
	<option name=":target">stm32l433ccu</option>
  </options>

  <modules>
    <module>modm:architecture:clock</module>
